{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "outDir": "dist",
    "types": ["webpack", "webpack-env"]
  },
  "include": ["index.ts"],
  "typeAcquisition": { "enable": true }
}
